{
  "name": "next-js-end-to-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "reset": "tsx prisma/reset.ts",
    "seed": "prisma db seed",
    "cy:dev": "cross-env NODE_ENV=test next dev --port 3100",
    "cy:open": "dotenv -e .env.test -- cypress open --e2e -b chrome",
    "cy:run": "dotenv -e .env.test -- cypress run --e2e -b chrome",
    "test:push": "dotenv -e .env.test -- prisma db push",
    "test": "cross-env NODE_ENV=test start-server-and-test cy:dev http://localhost:3100 cy:open",
    "test:run": "cross-env NODE_ENV=test start-server-and-test cy:dev http://localhost:3100 cy:run"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.1.0",
    "@hookform/resolvers": "^3.6.0",
    "@neondatabase/serverless": "^0.9.3",
    "@prisma/adapter-neon": "^5.14.0",
    "@prisma/client": "^5.16.1",
    "@prisma/extension-accelerate": "^1.1.0",
    "@types/styled-components": "^5.1.34",
    "argon2": "^0.40.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.18",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "react-icons": "^5.2.1",
    "styled-components": "^6.1.11",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "^13.11.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "prisma": "^5.16.1",
    "start-server-and-test": "^2.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2"
  }
}
